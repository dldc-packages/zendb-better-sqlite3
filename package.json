{
  "name": "zendb-better-sqlite3",
  "version": "2.0.0-5",
  "description": "A local database",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/zendb-better-sqlite3#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/zendb-better-sqlite3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/zendb-better-sqlite3.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/mod.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "example:run": "tsx ./example/index.ts",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn run build && np",
    "test": "yarn run lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "tsx": "^3.12.7"
  },
  "devDependencies": {
    "@swc/core": "^1.3.62",
    "@types/better-sqlite3": "^7.6.4",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "better-sqlite3": "^8.4.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "nanoid": "^4.0.2",
    "np": "^8.0.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "zendb": "^5.0.0-18"
  },
  "peerDependencies": {
    "better-sqlite3": "^8.4.0",
    "zendb": "5.0.0-18"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
